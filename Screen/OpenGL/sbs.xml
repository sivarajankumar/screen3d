<?xml version="1.0" encoding="UTF-8"?>
<pack>
	<properties>
		<name>Screen/OpenGL</name>
		<version>0.1.0</version>
		<buildtype>shared</buildtype>
	</properties>
	<main>
		<dependencies>
			<dependency name="Screen/Main" version="0.1.0"/>
			<dependency name="Screen/Utils" version="0.1.0"/>
			<dependency name="OpenGL">
				<includes>
					<path>Ext</path>
				</includes>
				<libraries>
					<lib>opengl32</lib>
					<lib>glu32</lib>
				</libraries>
			</dependency>
		</dependencies>
	</main>
	<descriptions>
		<library name="opengl32" type="static" compile-name="opengl32" />
		<library name="glu32" type="static" compile-name="glu32" />
	</descriptions>
</pack>
